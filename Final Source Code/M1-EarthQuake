    "from sklearn.preprocessing import LabelEncoder\n",
    "var_mod = ['magType', 'status', 'locationSource']\n",
    "le = LabelEncoder()\n",
    "for i in var_mod:\n",
    "    df[i] = le.fit_transform(df[i]).astype(int)"
   ]
  },
  {
   "cell_type": "code",
